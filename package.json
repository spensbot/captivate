{
  "name": "Captivate",
  "version": "1.0.0",
  "description": "Lighting and Visual Synth",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack/react.webpack.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.js",
    "build:react": "webpack --config webpack/react.webpack.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir",
    "test": "jest",
    "install": "electron-rebuild"
  },
  "keywords": [],
  "author": "Spenser Saling <spenser0saling@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^12.0.0",
    "@commitlint/core": "^9.0.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.11",
    "@types/serialport": "^8.0.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "electron": "^8.2.2",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-rebuild": "^2.3.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5",
    "wait-on": "^5.2.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/react-helmet": "^6.1.0",
    "babel-polyfill": "^6.26.0",
    "bindings": "^1.5.0",
    "caniuse-lite": "^1.0.30001183",
    "formik": "^2.2.6",
    "lodash.throttle": "^4.1.1",
    "nanoid": "^3.1.20",
    "node-link": "file:../../Dev/node-link",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-split": "^2.0.9",
    "react-split-pane": "^0.1.92",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.7",
    "serialport": "^9.0.2",
    "styled-components": "^5.1.1",
    "three": "^0.122.0",
    "yup": "^0.32.8"
  },
  "build": {
    "appId": "com.sailingtomars.captivate",
    "mac": {
      "category": "public.app-category.music"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
